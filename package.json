{
  "name": "my-nifty-collection",
  "version": "0.1.2",
  "description": "present my NFT to public",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "cd overlay && npm i",
    "start": "concurrently -c \"yellow,blue\" -n \"dapplet,overlay\" \"rollup -w --config rollup.config.js\" \"cd overlay && npm start\"",
    "build": "rollup --config rollup.config.js",
    "prettier": "npx prettier --write .",
    "linter": "npx eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dapplets/my-nifty-collection.git"
  },
  "cacheDirectories": [
    "node_modules",
    "overlay/node_modules"
  ],
  "author": "Project Dapplets",
  "license": "MIT",
  "devDependencies": {
    "@dapplets/dapplet-extension": "latest",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-typescript": "^8.1.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "concurrently": "^6.0.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "^2.2.1",
    "rollup": "^2.38.3",
    "rollup-plugin-serve": "^1.1.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@dapplets/dapplet-overlay-bridge": "^0.1.0"
  },
  "bugs": {
    "url": "https://github.com/dapplets/my-nifty-collection/issues"
  },
  "homepage": "https://github.com/dapplets/my-nifty-collection#readme"
}
